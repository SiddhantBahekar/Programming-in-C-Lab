#include<stdio.h>
void main() {
	int a,b;
	printf("Enter the first integer: ");
	scanf("%d", &a);
	printf("Enter the second integer: ");
	scanf("%d", &b);
	if(a == b) {
		printf("%d is equal to %d\n", a,b);
	}
	if(a>b) {
		printf("%d is greater than %d\n",a,b);
	}
	if(a<b) {
		printf("%d is less than %d\n", a,b);
	}
}
